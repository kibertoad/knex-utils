{
  "name": "knex-utils",
  "version": "4.1.7",
  "description": "knex.js utils",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "test": "mocha --opts \"./mocha.opts\" test/**/*.spec.js",
    "test:coverage": "nyc npm test",
    "test:ci": "npm run lint && nyc mocha --opts \"./mocha.opts\" test"
  },
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    },
    {
      "name": "Nikita Gedgaudas",
      "email": "gedgaudasnikita@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/knex-utils.git"
  },
  "homepage": "https://github.com/kibertoad/knex-utils",
  "dependencies": {
    "knex-tablecleaner": "^3.1.2",
    "validation-utils": "^4.0.2"
  },
  "peerDependencies": {
    "knex": " >= 0.14.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "knex": "^0.19.5",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "lib/**"
    ],
    "reporter": [
      "text"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "lines": 66,
    "statements": 66,
    "functions": 60,
    "branches": 45
  }
}
