{
  "name": "knex-utils",
  "version": "4.1.8",
  "description": "knex.js utils",
  "main": "src/index.js",
  "scripts": {
    "coveralls": "nyc report --reporter=lcov",
    "test": "mocha --opts \"./mocha.opts\" test/**/*.spec.js",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.js"
  },
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    },
    {
      "name": "Nikita Gedgaudas",
      "email": "gedgaudasnikita@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/knex-utils.git"
  },
  "homepage": "https://github.com/kibertoad/knex-utils",
  "dependencies": {
    "knex-tablecleaner": "^4.1.0",
    "validation-utils": "^6.0.0"
  },
  "peerDependencies": {
    "knex": " >= 0.14.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "knex": "0.19.5",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "sinon": "^7.5.0"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "lib/**"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "lines": 66,
    "statements": 66,
    "functions": 60,
    "branches": 45
  },
  "keywords": [
    "knex",
    "utils",
    "heartbeat",
    "clean",
    "where",
    "query",
    "build",
    "builder"
  ],
  "files": [
    "lib/*",
    "index.js",
    "LICENSE",
    "README.md"
  ]
}
